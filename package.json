{
  "name": "vscode-ungit",
  "displayName": "Ungit",
  "description": "Ungit in Visual Studio Code.",
  "icon": "images/logo.png",
  "version": "1.0.4",
  "publisher": "Hirse",
  "galleryBanner": {
    "color": "#252833",
    "theme": "dark"
  },
  "license": "MIT",
  "bugs": "https://github.com/hirse/vscode-ungit/issues",
  "homepage": "https://github.com/hirse/vscode-ungit",
  "repository": {
    "type": "git",
    "url": "https://github.com/hirse/vscode-ungit.git"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "ungit",
    "git"
  ],
  "engines": {
    "vscode": "^1.11.1"
  },
  "main": "./out/src/extension",
  "activationEvents": [
    "onCommand:extension.ungit"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.ungit",
        "title": "Open Ungit"
      }
    ],
    "keybindings": [
      {
        "command": "extension.ungit",
        "key": "shift+alt+u",
        "mac": "shift+alt+u"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -watch",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "lint": "tslint -p tsconfig.json",
    "pretest": "npm run lint",
    "test": "node ./node_modules/vscode/bin/test",
    "vscode:prepublish": "tsc"
  },
  "dependencies": {
    "ungit": "1.1.29"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.40",
    "@types/node": "^8.0.0",
    "mocha": "^4.0.0",
    "tslint": "^5.4.0",
    "typescript": "^2.3.0",
    "vsce": "^1.26.0",
    "vscode": "^1.1.0"
  }
}
