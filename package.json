{
  "name": "vscode-ungit",
  "displayName": "Ungit",
  "description": "Ungit in Visual Studio Code.",
  "icon": "images/logo.png",
  "version": "2.2.3",
  "publisher": "Hirse",
  "galleryBanner": {
    "color": "#252833",
    "theme": "dark"
  },
  "license": "MIT",
  "bugs": "https://github.com/hirse/vscode-ungit/issues",
  "homepage": "https://github.com/hirse/vscode-ungit",
  "repository": {
    "type": "git",
    "url": "https://github.com/hirse/vscode-ungit.git"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "ungit",
    "git",
    "multi-root ready"
  ],
  "engines": {
    "vscode": "^1.43.0"
  },
  "main": "./out/src/extension",
  "activationEvents": [
    "onCommand:extension.ungit"
  ],
  "contributes": {
    "configuration": {
      "title": "Ungit",
      "properties": {
        "ungit.showButton": {
          "type": "boolean",
          "default": false,
          "description": "Show a button in the editor title bar to open Ungit",
          "scope": "resource"
        },
        "ungit.showInActiveColumn": {
          "type": "boolean",
          "default": false,
          "description": "Open the Ungit view in the active editor column instead of on the side",
          "scope": "resource"
        }
      }
    },
    "commands": [
      {
        "command": "extension.ungit",
        "title": "Open Ungit",
        "icon": {
          "dark": "./images/icon-dark.svg",
          "light": "./images/icon-light.svg"
        }
      }
    ],
    "menus": {
      "editor/title": [
        {
          "when": "config.ungit.showButton",
          "command": "extension.ungit",
          "group": "navigation"
        }
      ]
    },
    "keybindings": [
      {
        "command": "extension.ungit",
        "key": "shift+alt+u",
        "mac": "shift+alt+u"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -watch",
    "package": "vsce package",
    "postinstall": "patch-package",
    "publish": "vsce publish",
    "test": "tslint -t verbose -p tsconfig.json",
    "vscode:prepublish": "tsc"
  },
  "dependencies": {
    "ungit": "1.5.6",
    "vscode-extension-telemetry": "0.1.2"
  },
  "devDependencies": {
    "@types/node": "12.x.x",
    "@types/vscode": "1.x.x",
    "patch-package": "6.x.x",
    "postinstall-postinstall": "2.x.x",
    "tslint": "6.x.x",
    "typescript": "3.8.x",
    "vsce": "1.x.x"
  },
  "greenkeeper": {
    "ignore": [
      "@types/node"
    ]
  }
}
