{
    "compileOnSave": true,
    "compilerOptions": {
        "lib": [
            "es6"
        ],
        "module": "commonjs",
        "noImplicitAny": true,
        "outDir": "out",
        "rootDir": ".",
        "sourceMap": true,
        "strictNullChecks": true,
        "target": "es6",
        "resolveJsonModule": true,
        "esModuleInterop": true
    },
    "exclude": [
        "node_modules"
    ]
}
